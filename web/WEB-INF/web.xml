    <servlet-mapping>
        <servlet-name>FillAreaByScribble</servlet-name>
        <url-pattern>/FillAreaByScribble</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
