    </servlet>
    <servlet>
        <servlet-name>FillAreaByScribble</servlet-name>
        <servlet-class>servlets.FillAreaByScribble</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FaceDetector</servlet-name>
        <url-pattern>/FaceDetector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindObject</servlet-name>
        <url-pattern>/FindObject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FillArea</servlet-name>
        <url-pattern>/FillArea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExtractText</servlet-name>
        <url-pattern>/ExtractText</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExtractTextFromImageObject</servlet-name>
        <url-pattern>/ExtractTextFromImageObject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindBlobs</servlet-name>
        <url-pattern>/FindBlobs</url-pattern>
    </servlet-mapping>
